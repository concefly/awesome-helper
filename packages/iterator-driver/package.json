{
  "name": "iterator-driver",
  "version": "6.0.4",
  "description": "Tiny 迭代器驱动",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "prettier": "prettier -c src",
    "lint": "npm run prettier",
    "test": "jest",
    "ci": "tsc -p tsconfig.build.es.json --noEmit && tsc -p tsconfig.build.json --noEmit && npm run test",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --no-watchman --coverage=false --testTimeout=60000",
    "clean": "rm -rf dist/*",
    "build:es": "tsc -p tsconfig.build.es.json --outDir dist/es",
    "build:lib": "tsc -p tsconfig.build.json --outDir dist/lib",
    "build": "npm run clean && npm run build:lib && npm run build:es",
    "prepublish": "npm run ci && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concefly/iterator-driver.git"
  },
  "author": "concefly@foxmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/concefly/iterator-driver/issues"
  },
  "homepage": "https://github.com/concefly/iterator-driver#readme",
  "devDependencies": {
    "@types/jest": "^24.0.14",
    "@types/node": "^12.7.1",
    "ah-prettier": "^2.1.0",
    "jest": "^24.8.0",
    "prettier": "^2.1.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "prettier": "ah-prettier",
  "sideEffects": false,
  "dependencies": {
    "ah-event-bus": "^1.0.3"
  }
}
